<% include partials/header %>

<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h1><%= blog.title %></h1>
      <div class="thumbnail">
        <img src="<%= blog.image %>">
      </div>
      <div class="caption">
        <strong><em>Created by Ryan</em></strong>
      </div>
      <div class="well">
        <h4><strong><%= blog.created.toDateString() %></strong></h4>
        <p><%= blog.body %></p>
      </div>
    </div>
  </div>
</div>

<% include partials/footer %>
